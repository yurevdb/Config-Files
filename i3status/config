general {
	output_format = "i3bar"
	colors = false
	markup = pango
	interval = 5
}

order += "volume master"
order += "time"

volume master {
	format = "<span background='#ebbcba'>  %volume  </span>"
	format_muted = "<span background='#ebbcba'>  Muted  </span>"
	device = "default"
	mixer = "Master"
	mixer_idx = 0
}

time {
	format = "<span background='#ebbcba'>  %H:%M - %b %d   </span>"
}

