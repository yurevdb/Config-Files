general {
	output_format = "i3bar"
	colors = false
	markup = pango
	interval = 5
}

order += "volume master"
order += "time"

volume master {
	format = "<span background='#eb6f92'>  %volume  </span>"
	format_muted = "<span background='#eb6f92'>  Muted  </span>"
	device = "default"
	mixer = "Master"
	mixer_idx = 0
}

time {
	format = "<span background='#eb6f92'>  %b %d at %H:%M  </span>"
}

